<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:gravity="center_horizontal"
	android:background = "@color/gray"
	android:orientation="vertical" >
      
	 <TextView
	     android:id = "@+id/market_garage_title"
	     android:layout_width = "wrap_content"
	     android:layout_height = "wrap_content"
	     android:textSize="16sp">
	 </TextView>
	    
	 <TextView
	     android:id = "@+id/market_garage_info"
	     android:layout_width = "wrap_content"
	     android:layout_height = "wrap_content"
	     android:textSize="14sp">
	 </TextView>
	
	 <TextView
	     android:id = "@+id/water_garage_title"
	     android:layout_width = "wrap_content"
	     android:layout_height = "wrap_content"
	     android:textSize="16sp">
	 </TextView>
	    
	 <TextView
	     android:id = "@+id/water_garage_info"
	     android:layout_width = "wrap_content"
	     android:layout_height = "wrap_content"
	     android:textSize="14sp">
	 </TextView>
	    
	 <TextView
	     android:id = "@+id/water_lot_title"
	     android:layout_width = "wrap_content"
	     android:layout_height = "wrap_content"
	     android:textSize="16sp">
	 </TextView>
	    
	 <TextView
	     android:id = "@+id/water_lot_info"
	     android:layout_width = "wrap_content"
	     android:layout_height = "wrap_content"
	     android:textSize="14sp">
	 </TextView>
	 
	 <TextView
	     android:id = "@+id/more_info_title"
	     android:layout_width = "wrap_content"
	     android:layout_height = "wrap_content"
	     android:textSize="16sp">
	 </TextView>
	    
	 <TextView
	     android:id = "@+id/more_info_info"
	     android:layout_width = "wrap_content"
	     android:layout_height = "wrap_content"
	     android:textSize="14sp">
	 </TextView>
	    
	 <TextView
	     android:id = "@+id/more_info_link"
	     android:layout_width = "wrap_content"
	     android:layout_height = "wrap_content"
	     android:textSize="14sp">
	 </TextView>
   
</LinearLayout>